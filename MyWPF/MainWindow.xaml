<Window x:Class="MyWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="label" Content="{Binding TestResult}" FontSize="64" HorizontalAlignment="Left" Margin="178,196,0,0" VerticalAlignment="Top" Height="106" Width="457"/>
        <Button x:Name="button" FontSize="32" Content="Click" Command="{Binding ClickCommand}" HorizontalAlignment="Left" Margin="70,39,0,0" VerticalAlignment="Top" Width="137" Height="90"/>

    </Grid>
</Window>
